{{@ lang('copyright')}}
<div class="beipy-video" {{ if radius }} style="border-radius:{{ radius }}" {{/if}}>
  <video src="https://blz-videos.nosdn.127.net/1/OverWatch/OVR_D.VA_SHOOTING_STAR_zhCN_YT_PC_3.mp4"></video>
  {{ if logo }}
  <div class="beipy-video-logo" {{if size=='min' }} style="height: 20px;" {{else if size=='middle' }}
    style="height: 30px;" {{/if}}>
    <img src="{{ logo }}" />
  </div>
  {{ /if }}
  <div class="beipy-video-controls">
    <!-- 控制背景遮罩 -->
    <div class="beipy-video-controls-mask"></div>
    <div class="beipy-video-controls-wrap">
      <div class="beipy-video-controls-wrap-footer">
        <!-- 左侧按钮组 -->
        <div class="beipy-video-controls-wrap-footer-left">
          <div class="beipy-video-controls-wrap-footer-btn-item play-icon ">
            {{@ icon.play.html}}
            {{@ icon.pause.html}}
          </div>

          {{each footer.left}}

          {{if $value.type=='loop'}}
          <div class="beipy-video-controls-wrap-footer-btn-item loop-icon-btn " {{if $value.show==false
            }}style="display:none ;" {{/if}}>
            {{@ icon.loopOff.html}}
            <!-- {{@ icon.loopOn.html}} -->
          </div>
          {{else if $value.type=='live'}}
          <div class="beipy-video-controls-wrap-footer-btn-item beipy-live-tag" {{if $value.show==false
            }}style="display:none ;" {{/if}}>
            <span></span> {{lang('live')}}
          </div>
          {{else if $value.type=='time'}}
          <div class="beipy-video-controls-wrap-footer-btn-item beipy-video-time" {{if $value.show==false }}
            style="display:none;" {{/if}}>
            <span>10:52</span><span> / </span><span>10:52</span>
          </div>
          {{else}}
          <div class="beipy-video-controls-wrap-footer-btn-item {{icon[$value.type].key}}" {{if $value.show==false
            }}style="display:none ;" {{/if}} title="{{lang('icon')[$value.type]}}">
            {{@ icon[$value.type].html}}
          </div>
          {{/if}}
          {{/each}}

        </div>


        <!-- 右侧按钮组 -->
        <div class="beipy-video-controls-wrap-footer-right">
          {{each footer.right}}
          {{if $value=='quality'}}
          <div class="beipy-video-controls-wrap-footer-btn-item beipy-video-tag-bg quality">
            <div class="beipy-tipMenu-box">
              <div class="beipy-Menu">
                <div class="beipy-Menu-item">
                  asdfas
                </div>
              </div>
            </div>
            {{lang('quality')}}
          </div>
          {{else if $value=='speed'}}
          <div class="beipy-video-controls-wrap-footer-btn-item beipy-video-tag-bg speed">
            {{lang('speed')}}
          </div>

          {{else}}
          <div class="beipy-video-controls-wrap-footer-btn-item {{icon[$value].key}}" title="{{lang('icon')[$value]}}">
            {{@ icon[$value].html}}
            <!-- <div>{{lang('icon')[$value]}}</div> -->
          </div>
          {{/if}}
          {{/each}}
        </div>
      </div>
      <!-- 进度条 -->
      <div class="beipy-video-controls-wrap-progress">
        <div class="progress-box">
          <!-- 预加载 -->
          <div class="progress-loading"></div>
          <!-- 当前播放进度 -->
          <div class="progress-currentloading"></div>
          <!-- 控制点 -->
          <div class="progress-highlight">
            <div class="progress-highlight-item"></div>
          </div>
          <!-- 控制点 -->
          <div class="progress-currentplay"></div>

        </div>
      </div>

    </div>
    <div class="beipy-video-controls-tip">
      <div class="beipy-video-controls-tip-msg">
        BeipyVideoJs演示demo视频
      </div>
    </div>
  </div>
  <!-- 工具箱 -->
  <div class="beipy-video-utools">
    {{each utools}}
    <div class="beipy-video-utools-item  {{icon[$value].key}}" title="{{lang('icon')[$value]}}">
      {{@ icon[$value].html}}
    </div>
    {{/each}}
  </div>
</div>